    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
